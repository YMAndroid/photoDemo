/* pages/me/me.wxss */

.me-page{
    display: flex;
    flex-direction: column;
    width: 100%;
    height: 100vh;
    background-color: rgb(231, 236, 241);
}

.item{
    background-color: white;
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    height: 50px;
    align-items: center;
    padding-left: 10rpx;
    padding-right: 10rpx;
    margin-top: 1px;
}

.other{
    margin-top: 10rpx;
    display: flex;
    flex-direction: column;
    background-color: white;
    padding-left: 10rpx;
    padding-top: 10rpx;
    padding-bottom: 10rpx;
}

.info{
    display: flex;
    flex-direction: row;
    width: 100%;
    height: 30px;
    align-items: center;
}

